          UNION ALL
            -- PARTICIPACIÓN DE INTERESES MINORITARIOS EN EL RESULTADO NETO
            SELECT 9 AS orden,
                   'Intereses Minoritarios' AS extra_level,
                   codigo_padre_padre,
                   'Intereses Minoritarios' AS descripcion_padre_padre,
                   codigo_padre,
                   CASE codigo_padre_padre
                      WHEN '496' THEN 'Resultados positivos'
                      ELSE 'Resultados negativos'
                   END
                      AS descripcion_padre,
                   codigo,
                   descripcion,
                   SUM (saldo) AS saldo,
                   SUM (saldo_anterior) AS saldo_anterior,
                   nivel,
                   cod_ufi,
                   'Participación de los intereses minoritarios en el resultado netro (*)'
                      AS label_sumatoria,
                   0 AS sumarXOperaciones,
                   0 AS mostrarXOperaciones,
                   0 AS sumarDistintoOperaciones,
                   1 AS noShow
              FROM (
                    SELECT h.codigo AS codigo_padre_padre,
                             g.codigo AS codigo_padre,
                             d.codigo,
                             d.descripcion,
                             SUM (saldos) AS saldo,
                             0 AS saldo_anterior,
                             d.nivel,
                             b.sk_cod_ufi_usefi AS cod_ufi
                        FROM tc_periodo_contable a,
                             tc_periodos_contables_saldos b,
