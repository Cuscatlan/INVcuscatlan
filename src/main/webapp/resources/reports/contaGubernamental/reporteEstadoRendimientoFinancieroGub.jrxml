                   0 AS noShow
              FROM (
                SELECT h.codigo AS codigo_padre_padre,
                             g.codigo AS codigo_padre,
                             d.codigo,
                             d.descripcion,
                             SUM (saldos) AS saldo,
                             0 AS saldo_anterior,
                             d.nivel,
