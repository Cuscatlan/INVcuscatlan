                      AS descripcion_padre_padre,
                   codigo_padre,
                   CASE codigo_padre_padre
                      WHEN '491' THEN 'Resultados positivos'
                      ELSE 'Resultados negativos'
                   END
                      AS descripcion_padre,
                   codigo,
