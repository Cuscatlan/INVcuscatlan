	<queryString>
		<![CDATA[SELECT orden,
         extra_level,
         codigo_padre_padre,
         descripcion_padre_padre,
         codigo_padre,
         descripcion_padre,
         codigo,
         tabla_di.descripcion,
         saldo,
         saldo_anterior,
         nivel,
         cod_ufi,
         f.descripcion as nombre_institucion,
         label_sumatoria,
         sumarDistintoOperaciones,
         sumarXOperaciones,
         mostrarXOperaciones,
         noShow
    FROM (  -- INGRESOS DE OPERACIONES
            SELECT 1 AS orden,
                   '' AS extra_level,
                   codigo_padre_padre,
                   'Ingresos de Operaciones' AS descripcion_padre_padre,
                   codigo_padre,
                   descripcion_padre,
                   codigo,
                   descripcion,
                   SUM (saldo) AS saldo,
                   SUM (saldo_anterior) AS saldo_anterior,
                   nivel,
                   cod_ufi,
                   'Total de Ingresos de Operaciones' AS label_sumatoria,
                   1 AS sumarXOperaciones,
                   0 AS mostrarXOperaciones,
                   0 AS sumarDistintoOperaciones,
                   0 AS noShow
              FROM (
              SELECT h.codigo AS codigo_padre_padre,
                             g.codigo AS codigo_padre,
                             g.descripcion AS descripcion_padre,
                             d.codigo,
                             d.descripcion,
                             SUM (saldos) AS saldo,
                             0 AS saldo_anterior,
                             d.nivel,
                             b.sk_cod_ufi_usefi AS cod_ufi
                        FROM tc_periodo_contable a,
                             tc_periodos_contables_saldos b,
                             cc_catalogo_cuenta d,
                             cc_catalogo_cuenta g,
                             cc_catalogo_cuenta h
                       WHERE     a.sk_periodo_contable = b.sk_periodo
                             AND b.sk_cuenta = d.sk_cuenta_contable
                             AND d.id_parent = g.sk_cuenta_contable
                             AND g.id_parent = h.sk_cuenta_contable
							 and   a.sk_periodo_contable in ( select sk_periodo_contable from TC_PERIODO_CONTABLE where sk_periodo_gubernamental = $P{sk_periodo_gubernamental})
                             AND (   d.codigo LIKE '411%'
                                  OR d.codigo LIKE '412%'
                                  OR d.codigo LIKE '413%'
                                  OR d.codigo LIKE '414%'
                                  OR d.codigo LIKE '415%'
                                  OR d.codigo LIKE '419%'
                                  OR d.codigo LIKE '421%'
                                  OR d.codigo LIKE '422%'
                                  OR d.codigo LIKE '429%'
                                  OR d.codigo LIKE '431%'
                                  OR d.codigo LIKE '432%'
                                  OR d.codigo LIKE '433%'
                                  OR d.codigo LIKE '434%'
                                  OR d.codigo LIKE '439%'
                                  OR d.codigo LIKE '441%'
                                  OR d.codigo LIKE '442%'
                                  OR d.codigo LIKE '443%'
                                  OR d.codigo LIKE '444%'
                                  OR d.codigo LIKE '451%'
                                  OR d.codigo LIKE '452%'
                                  OR d.codigo LIKE '461%'
                                  OR d.codigo LIKE '462%'
                                  OR d.codigo LIKE '492%'
                                  OR d.codigo LIKE '494%'
                                  OR d.codigo LIKE '499%')
                             AND d.nivel = 3
		        $P!{parameter_report}
                    GROUP BY d.codigo,
                             d.descripcion,
                             d.nivel,
                             b.sk_cod_ufi_usefi,
                             g.codigo,
                             g.descripcion,
                             h.codigo,
                             h.descripcion
                    UNION ALL
                      -- INGRESOS DE OPERACIONES. PERIODO ANTERIOR
                      SELECT h.codigo AS codigo_padre_padre,
                             g.codigo AS codigo_padre,
                             g.descripcion AS descripcion_padre,
                             d.codigo,
                             d.descripcion,
                             0 AS saldo,
                             SUM (saldos) AS saldo_anterior,
                             d.nivel,
                             b.sk_cod_ufi_usefi AS cod_ufi
