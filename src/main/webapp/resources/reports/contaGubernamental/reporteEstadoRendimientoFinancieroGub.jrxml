                             cc_catalogo_cuenta h
                       WHERE     a.sk_periodo_contable = b.sk_periodo
                             AND b.sk_cuenta = d.sk_cuenta_contable
                             AND d.id_parent = g.sk_cuenta_contable
                             AND g.id_parent = h.sk_cuenta_contable
