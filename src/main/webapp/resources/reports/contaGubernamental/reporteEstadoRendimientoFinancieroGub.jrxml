                   SUM (saldo_anterior) AS saldo_anterior,
                   nivel,
                   cod_ufi,
                   'Total de Gastos de Operaciones' AS label_sumatoria,
                   2 AS sumarXOperaciones,
                   1 AS mostrarXOperaciones,
                   0 AS sumarDistintoOperaciones,
                   0 AS noShow
              FROM (
              SELECT h.codigo AS codigo_padre_padre,
