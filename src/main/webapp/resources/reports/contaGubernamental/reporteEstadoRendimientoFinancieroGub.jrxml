	<field name="ORDEN" class="java.math.BigDecimal"/>
	<field name="EXTRA_LEVEL" class="java.lang.String"/>
	<field name="CODIGO_PADRE_PADRE" class="java.lang.String"/>
	<field name="DESCRIPCION_PADRE_PADRE" class="java.lang.String"/>
	<field name="CODIGO_PADRE" class="java.lang.String"/>
	<field name="DESCRIPCION_PADRE" class="java.lang.String"/>
	<field name="CODIGO" class="java.lang.String"/>
	<field name="DESCRIPCION" class="java.lang.String"/>
	<field name="SALDO" class="java.math.BigDecimal"/>
	<field name="SALDO_ANTERIOR" class="java.math.BigDecimal"/>
	<field name="NIVEL" class="java.math.BigDecimal"/>
	<field name="COD_UFI" class="java.math.BigDecimal"/>
	<field name="NOMBRE_INSTITUCION" class="java.lang.String"/>
	<field name="LABEL_SUMATORIA" class="java.lang.String"/>
	<field name="SUMARDISTINTOOPERACIONES" class="java.math.BigDecimal"/>
	<field name="SUMARXOPERACIONES" class="java.math.BigDecimal"/>
	<field name="MOSTRARXOPERACIONES" class="java.math.BigDecimal"/>
	<field name="NOSHOW" class="java.math.BigDecimal"/>
	<variable name="SALDO_ANTERIOR_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Nivel1" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO_ANTERIOR}]]></variableExpression>
	</variable>
	<variable name="SALDO_5" class="java.math.BigDecimal" resetType="Group" resetGroup="Nivel1" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO}]]></variableExpression>
	</variable>
	<variable name="SALDO_6" class="java.math.BigDecimal" resetType="Group" resetGroup="Nivel2" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO}]]></variableExpression>
	</variable>
	<variable name="SALDO_7" class="java.math.BigDecimal" resetType="Group" resetGroup="Nivel1" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO}]]></variableExpression>
	</variable>
	<variable name="resultDistintoOperaciones" class="java.math.BigDecimal" incrementType="Group" incrementGroup="Nivel1" calculation="Sum">
		<variableExpression><![CDATA[( ($F{SUMARDISTINTOOPERACIONES} == 1) ? $V{SALDO_5} : 0.0 )]]></variableExpression>
	</variable>
	<variable name="resultEjercicio" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{resultPorOperaciones}.subtract($V{resultDistintoOperaciones})]]></variableExpression>
	</variable>
	<variable name="resultPorOperaciones" class="java.math.BigDecimal" incrementType="Group" incrementGroup="Nivel1" calculation="Sum">
		<variableExpression><![CDATA[( ($F{SUMARXOPERACIONES} == 1) ? $V{SALDO_5} : ( ($F{SUMARXOPERACIONES} == 2)  ? $V{SALDO_5}.negate() : 0.0 ) )]]></variableExpression>
	</variable>
	<variable name="SALDO_1" class="java.math.BigDecimal" resetType="Group" resetGroup="extraLevel" calculation="Sum">
		<variableExpression><![CDATA[( ($F{DESCRIPCION_PADRE}.compareTo("Resultados negativos")) ? $F{SALDO} : $F{SALDO}.negate())]]></variableExpression>
	</variable>
	<variable name="interesesMinoritarios" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{SALDO_1}]]></variableExpression>
	</variable>
	<variable name="resultFinal" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{resultEjercicio}.add( $V{interesesMinoritarios} )]]></variableExpression>
	</variable>
	<variable name="SALDO_ANTERIOR_2" class="java.math.BigDecimal" resetType="Group" resetGroup="extraLevel" calculation="Sum">
		<variableExpression><![CDATA[( ($F{DESCRIPCION_PADRE}.compareTo("Resultados negativos")) ? $F{SALDO_ANTERIOR} : $F{SALDO_ANTERIOR}.negate())]]></variableExpression>
	</variable>
	<variable name="SALDO_ANTERIOR_3" class="java.math.BigDecimal" resetType="Group" resetGroup="Nivel2" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO_ANTERIOR}]]></variableExpression>
	</variable>
	<variable name="SALDO_ANTERIOR_4" class="java.math.BigDecimal" incrementType="Group" incrementGroup="Nivel1" calculation="Sum">
