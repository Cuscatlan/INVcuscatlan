                   nivel,
                   cod_ufi,
                   codigo_padre,
                   descripcion_padre,
                   codigo_padre_padre
          UNION ALL
            -- GASTOS DE OPERACIONES
            SELECT 2 AS orden,
                   '' AS extra_level,
