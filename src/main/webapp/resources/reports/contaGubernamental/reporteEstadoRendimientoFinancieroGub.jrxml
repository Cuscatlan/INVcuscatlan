							 and   a.sk_periodo_contable in ( select sk_periodo_contable from TC_PERIODO_CONTABLE where sk_periodo_gubernamental = $P{sk_periodo_gubernamental_ant})
                             AND (   d.codigo LIKE '59103%'
                                  OR d.codigo LIKE '59104%'
                                  OR d.codigo LIKE '59105%'
                                  OR d.codigo LIKE '49103%'
                                  OR d.codigo LIKE '49104%'
                                  OR d.codigo LIKE '49105%')
                             AND d.nivel = 4
		        $P!{parameter_report}
