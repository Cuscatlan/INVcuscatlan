                      AS descripcion_padre,
                   codigo,
                   descripcion,
                   SUM (saldo) AS saldo,
                   SUM (saldo_anterior) AS saldo_anterior,
                   nivel,
                   cod_ufi,
                   'Total de Resultados por deterioro y pérdidas de activos'
                      AS label_sumatoria,
                   0 AS sumarXOperaciones,
                   0 AS mostrarXOperaciones,
