	<queryString>
		<![CDATA[select reg.* from
(
select
b.n_orden_report
,'Fondo General' as fondo
,a.importe
,b.descripcion from
(
select
reg.sk_tipo_fondo
,reg.descripcion
,sum(importe) as importe
from
(
select
ing.sk_tipo_fondo
,mp.sk_medio_percepcion as descripcion
,mp.importe
from
tt_ingresos_medios_perc mp
,tt_ingresos ing
,ct_colecturias_finales cf
where
mp.sk_transaccion_ingreso=ing.sk_transaccion_ingreso
$P!{cadena}
$P!{P_COLECTURIA}
and sk_tipo_fondo=1
and mp.sk_medio_percepcion in(1,14,4,7,6)
)reg
group by descripcion,reg.sk_tipo_fondo
)a right join
(
select sk_medio_percepcion,descripcion,n_orden_report from ct_medio_percepcion where sk_medio_percepcion in(1,14,4,7,6)
)b
on
a.descripcion=b.sk_medio_percepcion


union all


select
b.n_orden_report,
'Fondos Ajenos en Custodia' as fondo
,a.importe
,b.descripcion from
(
select
reg.sk_tipo_fondo
,reg.descripcion
,sum(importe) as importe
from
(
select
--(select descripcion from ct_medio_percepcion where  sk_medio_percepcion= mp.sk_medio_percepcion) as descripcion
ing.sk_tipo_fondo
,mp.sk_medio_percepcion as descripcion
,mp.importe
from
tt_ingresos_medios_perc mp
,tt_ingresos ing
,ct_colecturias_finales cf
where
mp.sk_transaccion_ingreso=ing.sk_transaccion_ingreso
$P!{cadena}
$P!{P_COLECTURIA}
and sk_tipo_fondo=2
and mp.sk_medio_percepcion in(1,14,4,7,6)
)reg
group by descripcion,reg.sk_tipo_fondo
)a right join
(
select sk_medio_percepcion,descripcion,n_orden_report from ct_medio_percepcion where sk_medio_percepcion in(1,14,4,7,6)
)b
on
a.descripcion=b.sk_medio_percepcion


union all


select
b.n_orden_report,
'Fondo de Actividades Especiales' as fondo
,a.importe
,b.descripcion from
(
select
reg.sk_tipo_fondo
,reg.descripcion
,sum(importe) as importe
from
(
select
--(select descripcion from ct_medio_percepcion where  sk_medio_percepcion= mp.sk_medio_percepcion) as descripcion
ing.sk_tipo_fondo
,mp.sk_medio_percepcion as descripcion
,mp.importe
from
tt_ingresos_medios_perc mp
,tt_ingresos ing
,ct_colecturias_finales cf
where
mp.sk_transaccion_ingreso=ing.sk_transaccion_ingreso
$P!{cadena}
$P!{P_COLECTURIA}
and sk_tipo_fondo=3
and mp.sk_medio_percepcion in(1,14,4,7,6)
)reg
group by descripcion,reg.sk_tipo_fondo
)a right join
(
select sk_medio_percepcion,descripcion,n_orden_report from ct_medio_percepcion where sk_medio_percepcion in(1,14,4,7,6)
)b
on
a.descripcion=b.sk_medio_percepcion)reg
group by
fondo,n_orden_report,importe,descripcion
order by
fondo]]>
