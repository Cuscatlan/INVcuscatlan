	<queryString>
		<![CDATA[SELECT codigo,
  descripcion,
  SUM(saldo)          AS saldo,
  SUM(saldo_anterior) AS saldo_anterior,
  nombre_ufi,
  nombre_institucion,
  saldo_al,
  grupoOperacion,
  nombreGrupo,
  totalOp,
  nota_explicativa
