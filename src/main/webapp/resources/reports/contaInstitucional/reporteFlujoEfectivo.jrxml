  (SELECT d.codigo ,
    d.descripcion,
    SUM(saldos)   AS saldo,
    0             AS saldo_anterior,
    e.descripcion AS nombre_ufi,
    f.descripcion AS nombre_institucion,
    'Del 1º de Enero de '
    || TO_CHAR(MAX(a.fecha_fin_per_cont),'yyyy')
    || ' al '
    || TO_CHAR(MAX(a.fecha_fin_per_cont),'dd')
    || ' de '
    || trim(TO_CHAR(MAX(a.fecha_fin_per_cont), 'Month', 'nls_date_language=Spanish'))
    || ' de '
    || TO_CHAR(MAX(a.fecha_fin_per_cont),'yyyy')         AS saldo_al,
    'COBROSOP'                                           AS grupoOperacion,
    'FLUJOS DE EFECTIVO DE LAS ACTIVIDADES DE OPERACIÓN' AS nombreGrupo,
    'Flujos netos de efectivo por actividades de operación' totalOp,
     GET_NOTA_EXPLICATIVA(4,$P{skPeriodo},d.sk_cuenta_contable) AS nota_explicativa
  FROM tc_periodos_contables_saldos b
  LEFT JOIN tc_periodo_contable a ON a.sk_periodo_contable = b.sk_periodo
    LEFT JOIN  cc_catalogo_cuenta d ON d.sk_cuenta_contable = b.sk_cuenta
      LEFT JOIN   vtb_ci_unidad_financ e ON e.sk_unidad_financ = b.sk_cod_ufi_usefi AND  e.sk_unidad_financ= b.sk_cod_ufi_usefi
