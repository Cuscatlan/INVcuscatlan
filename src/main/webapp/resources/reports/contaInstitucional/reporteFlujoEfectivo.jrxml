   LEFT JOIN  cc_tipo_movimiento h ON  h.sk_Tipo_Movimiento = b.sk_Tipo_Movimiento
  LEFT JOIN     Cc_Tipos_Cuenta j ON j.sk_tipo_cuenta =  d.sk_tipo_cuenta
  WHERE
  d.nivel                 =5
  AND d.tipo_actividad        = 'OPERACION'
  AND h.codigo                ='TM1'
  AND b.sk_cod_ufi_usefi      = $P{skUfi}
  AND a.sk_periodo_contable   = $P{skPeriodo} $P!{skFuenteFinanc} $P!{skPrograma} $P!{skSubPrograma} $P!{skActividad} $P!{skProyecto} $P!{skObraTarea} $P!{skConvenio} $P!{skTipoPresup} $P!{skSubTipoPresup} $P!{skClasifOrgFinan}
